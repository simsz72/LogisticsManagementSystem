<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="165.0" prefWidth="183.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.LoginPage">
   <children>
      <TextField fx:id="loginField" layoutX="14.0" layoutY="14.0" promptText="login:" />
      <PasswordField fx:id="passwordField" layoutX="14.0" layoutY="49.0" promptText="password:" />
      <CheckBox fx:id="managerCheck" layoutX="14.0" layoutY="84.0" mnemonicParsing="false" text="Manager" />
      <Button layoutX="14.0" layoutY="121.0" mnemonicParsing="false" onAction="#validate" text="Log in" />
      <Button layoutX="111.0" layoutY="121.0" mnemonicParsing="false" onAction="#openRegistration" text="Sign up" />
   </children>
</AnchorPane>
