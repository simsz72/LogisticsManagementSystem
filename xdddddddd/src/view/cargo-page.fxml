<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="239.0" prefWidth="267.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.CargoPage">
   <children>
      <Label layoutX="62.0" layoutY="14.0" text="Cargo page">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <TextField fx:id="cargoNameField" layoutX="60.0" layoutY="68.0" promptText="cargoName" />
      <TextField fx:id="cargoHeightField" layoutX="60.0" layoutY="144.0" promptText="height" />
      <TextField fx:id="cargoWeightField" layoutX="59.0" layoutY="105.0" promptText="weight" />
      <Button layoutX="60.0" layoutY="194.0" mnemonicParsing="false" onAction="#cancelCargo" text="Cancel" />
      <Button fx:id="actionButton" layoutX="165.0" layoutY="194.0" mnemonicParsing="false" onAction="#validateCargo" text="Done" />
   </children>
</AnchorPane>
